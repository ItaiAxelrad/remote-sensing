  "bugs": {
    "url": "https://github.com/ItaiAxelrad/remote-sensing-data/issues"
  },
  "homepage": "https://github.com/ItaiAxelrad/remote-sensing-data#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0"
  }
}
